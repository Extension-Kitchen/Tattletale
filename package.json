{
  "name": "tattletale",
  "version": "1.0.0",
  "description": "A simple discord bot & chaster extension that forwards chaster lock events to users discord DM's",
  "type": "module",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "postinstall": "patch-package",
    "start": "wrangler pages dev",
    "ngrok": "ngrok http 8788",
    "test:watch": "vitest",
    "test": "vitest --watch false",
    "fix": "eslint --fix .",
    "typecheck": "npm run typecheck:src && npm run typecheck:tests",
    "typecheck:tests": "tsc --noEmit --project test",
    "typecheck:src": "tsc --noEmit --project .",
    "lint": "eslint . --config eslint.config.js",
    "register": "deno run --unstable-sloppy-imports --allow-env --allow-read --allow-net src/discord/register.ts",
    "fetch-api:discord": "openapi-typescript https://raw.githubusercontent.com/discord/discord-api-spec/main/specs/openapi.json -o ./src/discord/api.d.ts",
    "fetch-api:chaster": "deno run --allow-net --allow-write --allow-read src/chaster/fetch-api.ts && openapi-typescript /tmp/chaster/openapi.json -o ./src/chaster/api.d.ts",
    "fetch-apis": "npm run fetch-api:chaster && npm run fetch-api:discord",
    "prepare": "husky"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "discord-interactions": "^3.4.0",
    "openapi-fetch": "^0.9.7"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.4.4",
    "@cloudflare/workers-types": "^4.0.0",
    "@eslint/js": "^9.4.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.2",
    "deno-bin": "^1.44.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "ngrok": "^5.0.0-beta.2",
    "openapi-typescript": "^6.7.6",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vitest": "1.5.0",
    "wrangler": "^3.48.1"
  }
}

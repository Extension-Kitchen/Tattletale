{
	"compilerOptions": {
		//"strict": true,
		"module": "esnext",
		"target": "esnext",
		"lib": ["esnext"],
		"moduleResolution": "bundler",
		"noEmit": true,
		"skipLibCheck": true,
		"types": ["@cloudflare/workers-types"],
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"paths": {
			"@src/*": ["src/*"],
		}
	},
	"exclude": ["node_modules", ".direnv", ".wrangler", "test"],
}